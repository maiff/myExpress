{
  "prompts": {
    "name": {
      "type": "input",
      "required": true,
      "message": "Project name"
    },
    "description": {
      "type": "input",
      "required": false,
      "message": "Project description",
      "default": "A myExpress project"
    },
    "author": {
      "type": "string",
      "message": "Author"
    }
  },
  "functions": {
    "mongoose": {
      "type": "confirm",
      "message": "use mongoose?",
      "filterFile": "./connect",
      "promptsFile": "./connect/connect.js",
      "prompts": {
        "url": {
          "type": "input",
          "required": true,
          "message": "mongodb url",
          "default": "mongodb://127.0.0.1:12345/test"
        }
      }
    },
    "test": {
      "type": "confirm",
      "message": "use mongoose?",
      "filterFile": "./ttt",
      "promptsFile": "./ttt/test.js",
      "prompts": {
        "test": {
          "type": "input",
          "required": true,
          "message": "test test",
          "default": "tttttt"
        }
      }
    }
  }
}
